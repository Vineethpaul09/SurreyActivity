# Surrey Activity Booking - Environment Configuration
# Copy this file to .env and fill in your credentials

# Login Credentials
SURREY_EMAIL=your-email@example.com
SURREY_PASSWORD=your-password-here

# Browser Settings
HEADLESS=true
SLOW_MO=100

# Timeouts (in milliseconds)
NAVIGATION_TIMEOUT=5000
ACTION_TIMEOUT=3000

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true
LOG_DIR=./logs
